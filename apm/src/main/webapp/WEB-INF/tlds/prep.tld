	<shortname>prep</shortname>
	<info>PREP Custom Tag library</info>
	<!-- A Simple tag -->


	<tag>
		<name>canaccess</name>
		<tagclass>com.ascap.apm.controller.customtags.CanAccess</tagclass>
		<!--Body content can have a value of empty: no body JSP: body that is evaluated 
			by container, then possibly processed by the tag tagdependent: body is only 
			processed by tag; JSP in body is not evaluated. -->
		<bodycontent>empty</bodycontent>
		<info>This tag will provide role based hyperlinks.</info>
		<attribute>
			<name>href</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>anchortext</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>enableflag</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
