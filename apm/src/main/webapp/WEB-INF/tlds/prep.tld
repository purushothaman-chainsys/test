	<tag>
		<name>secure</name>
		<tagclass>com.ascap.apm.controller.customtags.SecureTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>This tag restricts the page level elements based on the User
			credentials</info>
		<attribute>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>




	<tag>
		<name>uiWidget</name>
		<tagclass>com.ascap.apm.controller.customtags.UIWidgetTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>This tag renders the page level elements based on the User
			credentials</info>
		<attribute>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
