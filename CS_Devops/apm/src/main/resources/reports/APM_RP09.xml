<csv-reformat>
<!-- 
	Sets the minimum number of columns there should be.  If there are fewer columns found
	in any line of the CSV file, it will throw an error
 -->
	<minimum-columns value="2"/>
<!-- 
	This sets the delimiter.  Usually if a string is in a CSV value, it's in double quotes
	in case there is a comma in the string.  There should be no need to change this
 -->
	<delimiter value=''/>
<!-- 
	This sets the separator.
 -->
	<separator value=","/>
<!-- 
	This sets the new separator.  If it's not specified, the separator is used
